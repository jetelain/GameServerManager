@model IEnumerable<LogVM>
@{
    ViewData["Title"] = "Audit";
    var server = (GameServer)ViewData["Server"];
}

@section NavBar {
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index">Serveurs</a></li>
            <li class="breadcrumb-item"><a asp-action="Details" asp-route-id="@server.GameServerID">@server.Label</a></li>
            <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
        </ol>
    </nav>
}

@section Title { <h1><span>@ViewData["Title"]</span></h1> }
<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Description</th>
            <th>Durée</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var process in Model)
        {
            <tr>
                <td>@process.Log.Timestamp</td>
                <td>@process.Label</td>
                <td>@process.Log.Duration</td>
            </tr>
        }
    </tbody>
</table>



<a name="endofpage"></a>